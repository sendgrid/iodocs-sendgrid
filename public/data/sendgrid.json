            "methods": [
                {
                    "MethodName": "Get",
                    "Synopsis": "Retrieve a list of Blocks with addresses and response codes",
                    "HTTPMethod": "GET",
                    "URI": "/blocks.get.:format",
                    "RequiresOAuth": "N",
                    "parameters": [
                        {
                            "Name": "date",
                            "Required": "N",
                            "Default": "1",
                            "Type": "int",
                            "Description": "Retrieve timestamp of the Block records in YYYY-MM-DD HH:MM:SS format"
                        },
                        {
                            "Name": "days",
                            "Required": "N",
                            "Default": "",
                            "Type": "int",
                            "Description": "Number of days in the past for which to retrieve blocks (includes today). Must be an integer greater than 0"
                        },
                        {
                            "Name": "start_date",
                            "Required": "N",
                            "Default": "",
                            "Type": "int",
                            "Description": "The start date of the range to look up statistics. Date must be in YYYY-MM-DD format and be before the end_date parameter"
                        },
                        {
                            "Name": "end_date",
                            "Required": "N",
                            "Default": "",
                            "Type": "int",
                            "Description": "The end date of the range to look up statistics. Date must be in YYYY-MM-DD format and be after the start_date parameter"
                        },
                        {
                            "Name": "format",
                            "Required": "N",
                            "Default": "json",
                            "Type": "enumerated",
                            "EnumeratedList": [
                                "json",
                                "xml"
                            ],
                            "Description": "Response format"
                        }
                    ]
